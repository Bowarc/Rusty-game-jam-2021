# EditorConfig file: https://EditorConfig.org
# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
[*.{rs,toml,md}]
charset = utf-8

# Tab indentation (no size specified)
[*.{rs,toml}]
indent_style = tab

[*.md]
indent_style = space
indent_size = 4
